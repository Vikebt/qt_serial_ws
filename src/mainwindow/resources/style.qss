/**********combobox**********/
QComboBox {
	min-height:35px;
	min-width:70px;
	background-color: rgb(111, 156, 207);
	border-radius: 5px;
	border: 0px solid gray; 
	color: rgb(255, 255, 255);
	}
QComboBox {
	combobox-popup:0;
	}
QComboBox:hover{
	background-color: rgb(52, 101, 164);
	}
QComboBox QAbstractItemView {
	outline: 0px solid gray;
	border: 1px solid #fdfdfd;
	color: rgb(255, 255, 255);
	background-color: #c4d2d5;
	selection-background-color: rgb(52, 101, 164);
	}
QComboBox QAbstractItemView::item{
	height:35px;
	color: #000000;
	}
QComboBox QAbstractItemView::item:selected { 
	color: #bbbbbb;
	background-color: rgb(52, 101, 164);
	}
QComboBox QAbstractItemView::item:hover 
	{color: #bbbbbb;
	background-color: rgb(52, 101, 164);
	}
QComboBox::drop-down {
	subcontrol-origin: padding;
 	subcontrol-position: top right;
 	height:35px;
 	width:29px;
 	border-left-width: 0px;
 	}
QComboBox::drop-down:hover {
	background: rgb(52, 101, 164);
}
QComboBox::down-arrow 
{
	height:35px;width:29px;image: url(:/resources/lightblue/add_bottom.png);
}
QComboBox::down-arrow:hover 
{
	image: url(:/resources/lightblue/add_bottom.png);
}
QComboBox::down-arrow:pressed {
	image: url(:/resources/lightblue/add_bottom.png);
}

/**********Î¢µ÷Æ÷**********/
QSpinBox {
        border-radius: 4px;
        height: 24px;
        min-width: 40px;
        border: 1px solid rgb(111, 156, 207);
        background: white;
}
QSpinBox:enabled {
        color: rgb(60, 60, 60);
}
QSpinBox:enabled:hover, QSpinBox:enabled:focus {
        color: rgb(51, 51, 51);
}
QSpinBox:!enabled {
        color: rgb(210, 210, 210);
        background: transparent;
}
QSpinBox::up-button {
        border-left: 1px solid rgb(111, 156, 207);
        width: 18px;
        height: 12px;
        border-top-right-radius: 4px;
        image: url(:/resources/lightblue/add_top.png);
}
QSpinBox::up-button:!enabled {
        background: transparent;
}

QSpinBox::up-button:enabled:hover {
        background: rgb(111, 156, 207);
}
QSpinBox::down-button {
        border-left: 1px solid rgb(111, 156, 207);
        width: 18px;
        height: 12px;
        border-bottom-right-radius: 4px;
        image: url(:/resources/lightblue/add_bottom.png);
}

QSpinBox::down-button:!enabled {
        background: transparent;
}
QSpinBox::down-button:hover {
        background: rgb(111, 156, 207);
}


QPushButton{
        border-radius: 4px;
        /*border: 2px solid #8f8f91;*/
        border-top: none;
    	border-left: none;
    	border-right: 2px solid #8f8f91;
    	border-bottom: 2px solid #8f8f91;
        width: 80px;
        height: 35px;
}
QPushButton:enabled {
        background: rgb(120, 170, 220);
        color: white;
}
QPushButton:!enabled {
        background: rgb(180, 180, 180);
        color: white;
}
QPushButton:enabled:hover{
        background: rgb(100, 160, 220);
}
QPushButton:enabled:pressed{
        background: rgb(0, 78, 161);
}


QPushButton:enabled:checked {
    background: rgb(0, 78, 161); 
    border-right: 2px solid #5a5a5a;
    border-bottom: 2px solid #5a5a5a;
}

